<h1>Listing photos</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Farm</th>
    <th>Server</th>
    <th>Secret</th>
    <th>Owner</th>
    <th>Photogenic</th>
    <th>Photogenic type</th>
    <th>Flickr</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.url %></td>
    <td><%= photo.farm %></td>
    <td><%= photo.server %></td>
    <td><%= photo.secret %></td>
    <td><%= photo.owner %></td>
    <td><%= photo.photogenic_id %></td>
    <td><%= photo.photogenic_type %></td>
    <td><%= photo.flickr_id %></td>
    <td><%= photo.title %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
